{
  "name": "wordle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check:next": "next lint -d src",
    "lint:check:style": "stylelint \"**/*.{css,scss,sass}\"",
    "lint:check:prettier": "yarn prettier . --check",
    "lint:fix:next": "next lint -d src --fix",
    "lint:fix:style": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:fix:prettier": "yarn prettier . --write ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.58.2",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.1.6",
    "husky": "^8.0.3",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@next/eslint-plugin-next": "^13.1.6",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "commitlint": "^17.4.4",
    "commitlint-config-gitmoji": "^2.3.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-prettier": "^2.0.0",
    "tailwind-merge": "^1.9.1",
    "tailwindcss": "^3.2.7"
  }
}
